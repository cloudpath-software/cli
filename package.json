{
  "name": "@cloudpath/cli",
  "description": "Cloudpath command line tool",
  "version": "0.1.0",
  "author": "Cloudpath Inc.",
  "type": "module",
  "engines": {
    "node": "^14.18.0 || >=16.4.0"
  },
  "files": [
    "dist",
    "/scripts/postinstall.mjs"
  ],
  "bugs": {
    "url": "https://github.com/cloudpath-software/cli/issues"
  },
  "homepage": "https://github.com/cloudpath-software/cli",
  "repository": "cloudpath/cli",
  "bin": {
    "cloudpath": "./dist/run.mjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "node ./dist/run.mjs",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "run-s format test:dev",
    "format": "run-s format:check-fix:*",
    "format:ci": "run-s format:check:*",
    "format:check-fix:lint": "run-e format:check:lint format:fix:lint",
    "format:check:lint": "cross-env-shell eslint $npm_package_config_eslint",
    "format:fix:lint": "cross-env-shell eslint --fix $npm_package_config_eslint",
    "format:check-fix:prettier": "run-e format:check:prettier format:fix:prettier",
    "format:check:prettier": "cross-env-shell prettier --check $npm_package_config_prettier",
    "format:fix:prettier": "cross-env-shell prettier --write $npm_package_config_prettier",
    "test:dev": "run-s certs test:init:* test:dev:*",
    "test:init": "run-s test:init:*",
    "test:init:cli-version": "npm run start -- --version",
    "test:init:cli-help": "npm run start -- --help",
    "test:init:eleventy-deps": "npm ci --prefix tests/integration/eleventy-site --no-audit",
    "test:init:hugo-deps": "npm ci --prefix tests/integration/__fixtures__/hugo-site --no-audit",
    "test:dev:ava": "ava --verbose",
    "test:dev:vitest": "vitest run tests/unit/ && vitest run tests/integration",
    "test:ci:ava:integration": "c8 -r json ava --concurrency 1 --no-worker-threads tests/integration/",
    "test:ci:vitest:unit": "vitest run --coverage tests/unit/",
    "test:ci:vitest:integration": "vitest run --coverage tests/integration/",
    "test:affected": "node ./tools/affected-test.mjs",
    "e2e": "node ./tools/e2e/run.mjs",
    "watch": "c8 --reporter=lcov ava --watch",
    "certs": "openssl req -x509 -out localhost.crt -keyout localhost.key -newkey rsa:2048 -nodes -sha256 -subj \"/CN=localhost\" -extensions EXT -config certconf"
  },
  "keywords": [
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "@cloudpath/js-api": "^0.0.8",
    "@cloudpath/shared-models": "^0.0.7",
    "@netlify/config": "^20.9.0",
    "@netlify/framework-info": "^9.8.5",
    "backoff": "^2.5.0",
    "better-opn": "^3.0.2",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "ci-info": "^3.8.0",
    "clean-deep": "^3.4.0",
    "cli-progress": "^3.12.0",
    "commander": "^10.0.1",
    "configstore": "^6.0.0",
    "debug": "^4.3.4",
    "decache": "^4.6.1",
    "dot-prop": "^8.0.0",
    "env-paths": "^3.0.0",
    "envinfo": "^7.8.1",
    "execa": "^7.1.1",
    "figlet": "^1.6.0",
    "find-up": "^6.3.0",
    "flush-write-stream": "^2.0.0",
    "folder-walker": "^3.2.0",
    "formdata-node": "^5.0.1",
    "from2-array": "^0.0.4",
    "get-port": "^6.1.2",
    "git-repo-info": "^2.1.1",
    "gitconfiglocal": "^2.1.0",
    "hasha": "^5.2.2",
    "https-proxy-agent": "^5.0.1",
    "inquirer": "^9.1.5",
    "is-wsl": "^2.2.0",
    "log-symbols": "^5.1.0",
    "node-version-alias": "^3.4.1",
    "ora": "^6.3.0",
    "p-map": "^6.0.0",
    "p-wait-for": "^5.0.2",
    "parallel-transform": "^1.2.0",
    "parse-github-url": "^1.0.2",
    "parse-gitignore": "^2.0.0",
    "prettyjson": "^1.2.5",
    "process": "^0.11.10",
    "string-similarity": "^4.0.4",
    "tabtab": "^3.0.2",
    "tempy": "^3.0.0",
    "terminal-link": "^3.0.0",
    "through2-filter": "^3.0.0",
    "through2-map": "^3.0.0",
    "update-notifier": "^6.0.2",
    "uuid": "^9.0.0",
    "wait-port": "^1.0.4"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/backoff": "^2.5.2",
    "@types/cli-progress": "^3.11.2",
    "@types/debug": "^4.1.8",
    "@types/envinfo": "^7.8.1",
    "@types/flush-write-stream": "^1.0.0",
    "@types/folder-walker": "^3.2.0",
    "@types/inquirer": "^9.0.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.0",
    "@types/parallel-transform": "^1.1.1",
    "@types/parse-github-url": "^1.0.0",
    "@types/parse-gitignore": "^1.0.0",
    "@types/prettyjson": "^0.0.30",
    "@types/string-similarity": "^4.0.0",
    "@types/through2-map": "^3.0.1",
    "@types/update-notifier": "^6.0.4",
    "@types/uuid": "^9.0.2",
    "@types/write-file-atomic": "^4.0.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@4.0.1"
}
